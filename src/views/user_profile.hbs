  <h3> <a > Hi{{username}}</a></h3>

  <ul class="list">
         {{#each res}}
         <li class="item">
             <a class="link" href="/users/{{id}}">
                 <img class="image" src="{{this.img}}">
             </a>
             <div class="info">
                 <h2>{{this.review}}</h2>
                 <h4>{{this.rate}}</h2>
             </div>
         </li>
         {{/each}}
     </ul>
