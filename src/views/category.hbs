<section class="category-section">
  <h2><span>top</span> Options for your choice</h2>
<div class="cat-result">
  {{#each response}}
  <div class="catResultPic" >
    <img class="cat-photo" id="{{name}}" src="{{img}}">
  </div>
  <div class="cat-data">
          <h2 class="cat-name">{{name}}</h2>
          <span class="fa fa-star checked"></span>
          <span class="fa fa-star checked"></span>
          <span class="fa fa-star checked"></span>
          <span class="fa fa-star"></span>
          <span class="fa fa-star"></span>
        </div>
        <h2 class="cat-name">{{name}}</h2>
        <div class="avg-evaluation">
            <h3>Avg Evaluation : {{avg}}/5 </h3>
        </div>
      </div>
  {{/each}}
</div>
</section>
